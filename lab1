    scanf("%d", &n);
    int mas[n];
    srand(time(0));
    printf("Сгенерированный массив:\n");
    for (i = 0; i < n; i++) {
        mas[i] = rand() % 100; // диапазон случайных чисел от 0 до 100





// counting sort
function CountingSort(mas[0:n - 1], B[0:n - 1], k){
 for (i = 0; i<k; i++){
    C[i] = 0
 }
 for (i = 0; i < n - 1; i++){
    C[mas[i]] = C[mas[i]] + 1}

 for (i = 1; i < k; i++){
    C[i] = C[i] + C[i - 1]}

 for (i = n - 1; i < 0; i++) {
    C[mas[i]] = C[mas[i]] - 1
    B[C[mas[i]]] = mas[i]}
}
 

// Bubble sort

function BubbleSort(mas[1:n], n)
 swapped == True

 while (swapped != False)
    for (i = 2; i < n; i++) {
        if (mas[i - 1] > mas[i]){
            swap(mas[i - 1], mas[i]){
                swapped = True}
        }
    }
}

// Merge sort
function MergeSort(mas[1:n], low, high){
    if (low < high)
    mid = floor((low + high) / 2)
 MergeSort(A, low, mid)
 MergeSort(A, mid + 1, high)
 Merge(A, low, mid, high)
    }
    

    printf("%d ", mas[i]);
    putchar('\n');
    system("pause");
    return 0;
}
